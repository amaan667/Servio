  "engines": {
    "node": ">=20.x",
    "pnpm": ">=9.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "NODE_NO_WARNINGS=1 next build",
    "start": "next start -p ${PORT:-8080}",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --config vitest.coverage.config.ts",
    "test:coverage:ui": "vitest --ui --coverage --config vitest.coverage.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:api": "swagger-jsdoc -d docs/swagger.config.js -o api-docs.json",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:create": "tsx scripts/create-migration.ts",
    "prepare": "husky install",
    "validate": "pnpm run format:check && pnpm run lint && pnpm run typecheck && pnpm run test"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,css,md,json}": [
      "prettier -w"
    ]
  },
