{
  "name": "servio-mvp",
  "version": "0.1.6",
  "private": true,
  "_deploy_trigger": "2026-02-18-mobile-session-fix",
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=9.x"
  },
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "dev:clean": "rm -rf .next && next dev",
    "build": "NODE_NO_WARNINGS=1 next build",
    "build:analyze": "ANALYZE=true NODE_NO_WARNINGS=1 next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --config vitest.coverage.config.ts",
    "test:coverage:ui": "vitest --ui --coverage --config vitest.coverage.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:api": "swagger-jsdoc -d docs/swagger.config.js -o api-docs.json",
    "docs:openapi": "tsx scripts/generate-openapi.ts",
    "docs:swagger": "echo 'Swagger UI available at /api/docs/swagger'",
    "migrate": "tsx scripts/run-migrations.ts",
    "migrate:create": "tsx scripts/create-migration.ts",
    "migrate:auto": "tsx scripts/run-migrations.ts",
    "migrate:prod": "NODE_ENV=production tsx scripts/run-migrations.ts",
    "cache:clear": "tsx scripts/clear-cache.ts",
    "cache:stats": "tsx scripts/cache-stats.ts",
    "perf:analyze": "tsx scripts/analyze-performance.ts",
    "health:check": "tsx scripts/health-check.ts",
    "cleanup:console-logs": "tsx scripts/cleanup-console-logs.ts",
    "track:todos": "tsx scripts/track-todos.ts",
    "prepare": "husky install",
    "check:rls": "tsx scripts/check-rls-coverage.ts",
    "check:api-policy": "node scripts/check-api-route-policy.mjs",
    "check:schema-contract": "node scripts/check-schema-contract.mjs",
    "test:security-critical": "vitest run __tests__/middleware.test.ts __tests__/security/api-route-policy.test.ts __tests__/security/unified-handler-auth.test.ts __tests__/security/idempotency-db.test.ts",
    "validate": "pnpm run typecheck && pnpm run check:rls && pnpm run check:api-policy && pnpm run check:schema-contract && pnpm run test:security-critical",
    "test:all": "pnpm run test && pnpm run test:e2e && pnpm run test:integration"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{js,jsx,css,md,json}": [
      "prettier -w"
    ]
  },
